<div class="columns large-6 small-centered large-uncentered competition" id="{compId}">
	<div class="title">{title}</div>
	{#entrants}
	<div class="entrant {id}">
		<div class="name"><a href="https://github.com/{id}" class="competition-link">{name} '{year} <span class="user-id">({id})</span></a></div>
		<div class="score">{streakVal} {label}</div>
	</div>{~n}
	{/entrants}
</div>