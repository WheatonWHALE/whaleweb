<div class="columns large-6 small-centered large-uncentered competition" id="{compId}">
	<div class="title">{title}</div>
	{#entrants}
	<div class="entrant {id}">
		<a href="https://github.com/{id}" class="competition-link"><div class="name">{name} <span class="user-id">({id})</span></div></a>
		<div class="score">{streakVal} {label}</div>
	</div>{~n}
	{/entrants}
</div>